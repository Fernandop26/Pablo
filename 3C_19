#include <iostream>
#include <stdlib.h>

using namespace std;
int main()
{
	int num1, num2, r0, num;

	num = 1;
	while (num!=0)
	{
		cin >> num;

		if ((num != 0) && (num % 2 == 0))
			num2 = 2;
		else
		{
		   num1 = 1;
			if (num == 0)
				r0 = 1;
		}
	}

	if (num2 == 2)
		cout << "Algun nombre de la sequencia es parell." << endl;

	else
	{
		cout << "No hi ha cap nombre parell en la sequencia" << endl;
		if ((r0 == 1) && (num1 == 0) && (num2 == 0))
			cout << "No hi ha cap nombre parell en la sequencia" << endl;
	}
	return 0;
}
